{
  "name": "Webpage Screenshot Extension",
  "description": "Capture a screenshot of the current webpage.",
  "version": "0.2",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "128": "images/logo.png"
    },
    "default_popup": "popup.html",
    "default_title": "MetaMask"
  },
  "icons": {
    "128": "images/logo.png"
  },
  "permissions": ["activeTab", "downloads"],
  "host_permissions": ["*://*/*"]
}
